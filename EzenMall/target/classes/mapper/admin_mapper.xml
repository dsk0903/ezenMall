<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ezen.mall.spring.admin">
	<select id="memberSelectAll" resultType="AdminDTO">
		select member_id, member_pwd, member_name from member
	</select>
	
	<select id="productSelectAll" resultType="AdminDTO">
		select product_number, product_name, product_price from product
	</select>
</mapper>